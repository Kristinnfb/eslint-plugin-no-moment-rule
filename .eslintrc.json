{
    "rules": {
        "@typescript-eslint/no-unused-vars": "off"
    },
    "ignorePatterns": [
        "node_modules/**"
    ],
    "overrides": [
        {
            "files": [
                "**/*.{js,mjs,cjs}"
            ],
            "env": {
                "node": true,
                "browser": true
            },
            "parserOptions": {
                "ecmaVersion": 2022,
                "sourceType": "module"
            },
            "plugins": [
                "no-moment"
            ],
            "rules": {
                "no-moment/no-moment": "error"
            }
        }
    ]
}